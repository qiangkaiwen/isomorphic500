{
  "ext_libs": {
    "ResourceLib": "/ext_libs/ResourceLib.js"
  },
  "mockResponses": {
    "default": {

      "/users": {
        "get": {
          "type": "function",
          "fn": "ResourceLib.sendAllUsers"
        }
      },

      "/users/:username": {
        "get": {
          "type": "function",
          "fn": "ResourceLib.sendUser"
        }
      },

      "/photos": {
        "get": {
          "type": "function",
          "fn": "ResourceLib.sendAllPhotos"
        }
      },

      "/photos/:id": {
        "get": {
          "type": "function",
          "fn": "ResourceLib.sendPhoto"
        }
      },

      "/static/:name": {
        "get": {
          "type": "function",
          "fn": "ResourceLib.sendAsset"
        }
      }
    }
  }
}
